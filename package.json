{
  "name": "@warp-ds/icons",
  "type": "module",
  "version": "0.0.1",
  "description": "WARP's icon set",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./index.js",
    "./react": "./react/index.js",
    "./vue": "./vue/index.js",
    "./elements": "./elements/index.js",
    "./elements/*": "./elements/*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "import": "node ./scripts/import.js",
    "build": "node ./scripts/build.js",
    "dev": "vite",
    "vite:build": "vite build",
    "preview": "pnpm run build && vite build && vite preview",
    "build:react": "rimraf react && mkdir react && node ./scripts/output/react.js && node ./react.eik.js",
    "build:vue": "rimraf vue && mkdir vue && node ./scripts/output/vue.js && node ./vue.eik.js",
    "build:elements": "rimraf elements && mkdir elements && node ./scripts/output/elements.js && node ./elements.eik.js"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@eik/esbuild-plugin": "^1.1.35",
    "@sindresorhus/slugify": "^2.1.0",
    "@warp-ds/uno": "^1.0.0",
    "camelcase": "^8.0.0",
    "chalk": "^5.0.1",
    "esbuild": "^0.14.42",
    "fs-extra": "^11.1.1",
    "glob": "^10.0.0",
    "happy-dom": "^10.0.0",
    "js-yaml": "^4.1.0",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.2.9",
    "ora": "^7.0.1",
    "prompts": "^2.4.0",
    "rimraf": "^5.0.1",
    "svgo": "^3.0.0",
    "unocss": "^0.55.2",
    "vite": "^4.4.9",
    "vite-plugin-nunjucks": "^0.1.10"
  },
  "publishConfig": {
    "access": "public"
  }
}